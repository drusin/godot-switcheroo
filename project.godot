; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Godot-Switcheroo"
run/main_scene="res://src/main.tscn"
config/features=PackedStringArray("4.0", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

CONSTANTS="*res://src/autoloads/constants.gd"
PREFERENCES="*res://src/autoloads/preferences.gd"
PROJECTS="*res://src/autoloads/projects.gd"
INSTALLATIONS="*res://src/autoloads/installations.gd"
DOWNLOAD_REPOSITORY="*res://src/autoloads/download_repository.gd"

[display]

window/subwindows/embed_subwindows=false

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[filesystem]

import/blender/enabled=false

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
